{
  "name": "inutile",
  "version": "0.3.0",
  "description": "Another useless Javascript utility functions collection",
  "main": "dist/inutile.esm.js",
  "module": "dist/inutile.esm.js",
  "browser": "dist/inutile.js",
  "scripts": {
    "dev": "rollup -c -w",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint src/**/*.js --max-warnings=0 --fix",
